<head>
  <title><%= content_for?(:title) ? content_for(:title) : "#{t :Peerstudio_title}" %></title>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta property="og:title" content="<%= content_for?(:title) ? content_for(:title) : "Stanford | PeerStudio" %>"/>
    <meta property="og:type" content="cause"/>
    <meta property="og:url" content="<%= "#{request.protocol}#{request.host_with_port}#{request.fullpath}" %>"/>
    <meta property="og:site_name" content="peerstudio"/>
    <meta property="og:description" content="<%= (@project && !@project.description.blank?) ? @project.description.truncate(100) : "Peer assessment for the small and massive classroom" %>" />
    <meta name="keywords" content="peer assessment, peer grading, online education, in class writing, writing assignment, gradebook, online gradebook, gradebook software, free gradebook software"><meta name="description" content="Free, easy to use Peer assessment for the small and massive classroom. Tools for students and teachers alike.">
    <script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-35198919-5', 'auto');
  <% if user_signed_in? %>
    ga('create', 'UA-XXXX-Y', { 'userId': '<%= non_pii_user_id %>' });
  <% end %>
  ga('require', 'displayfeatures');
  ga('send', 'pageview');

</script>
</head>